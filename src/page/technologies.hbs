---
section: technologies
---

{{!-- Header --}}
<header class="page-header inner-wrap" role="banner">
  {{> header_wocat}}
</header>

{{!-- Advanced search --}}
<aside id="search-advanced" class="form-search-advanced">
  {{> form_search_advanced}}
</aside>

{{!-- Body --}}
<main class="row large-collapse" role="main">
  <section class="tech-list small-12 columns">
    <div class="slider-container">
      <div class="nstSlider" data-range_min="2000" data-range_max="2015"
                             data-cur_min="2014"    data-cur_max="2015">
        <div class="bar"></div>
        <div class="leftGrip"></div>
        <div class="rightGrip"></div>
      </div>
      <span class="leftLabel">2014</span>
      <span class="rightLabel">2015</span>
  </div>

    <div class="tech-group">
      <h2 class="is-group">All technologies</h2>
      {{#each technology}}
        {{> tech_item}}
      {{/each}}
    </div>
  </section>
</main>